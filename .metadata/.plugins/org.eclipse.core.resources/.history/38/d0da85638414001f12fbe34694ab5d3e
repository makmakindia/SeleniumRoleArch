package Collections;

import java.util.ArrayList;
import java.util.List;

public class ArrayListDemo {
	
	public static void main(String[] args) {
		
		List list= new ArrayList();
		
		list.add(10);
		list.add("New York");
		list.add(true);
		list.add(10.34);
		list.add("New York");
		list.add(null);
		
		
		System.out.println(list);
		
	}

}
